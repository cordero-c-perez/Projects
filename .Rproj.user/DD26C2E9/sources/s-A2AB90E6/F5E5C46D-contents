library(tidyverse)
library(igraph)
library(gcookbook)

full_file <- read_csv("corpdoc.csv", col_names = TRUE)
full_file$`Current Entity Name` <-  str_to_lower(full_file$`Current Entity Name`)
colnames(full_file) <- new_names
full_file$current_entity_name <- str_to_lower(full_file$current_entity_name)

#sample network graph
g <- graph.data.frame(madmen, directed=FALSE)
par(mar=c(0,0,0,0))
plot(g, layout=layout.circle, vertex.size=8, vertex.color = "skyblue", vertex.frame.color = "green")
